[color]
  ui = auto
[alias]
  ci = commit -v
  co = checkout
  st = status
  sup = submodule update --init --recursive
  poh = push origin head
  poof = push origin head -f
  kick = commit --amend --no-edit --allow-empty
  cane = commit --amend --no-edit
  pr = pull --rebase
  unstage = restore --staged
[core]
  excludesfile = ~/.gitignore
  autocrlf = input
[merge]
  ff = only
[fetch]
  prune = true
[rebase]
  autosquash = true
